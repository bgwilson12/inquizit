generator client {
  provider = "prisma-client-js"
}

generator zod {
  provider = "zod-prisma-types"
  output   = "../src/zod"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Quiz {
  id        String   @id @default(uuid())
  quizName  String
  questions Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("quiz")
}
